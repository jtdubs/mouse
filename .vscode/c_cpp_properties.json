{
  "configurations": [
    {
      "name": "Linux",
      "defines": [
        "__AVR__",
        "__AVR_ATmega4809__",
        "F_CPU=16000000UL"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c23",
      "cppStandard": "c++23",
      "intelliSenseMode": "linux-gcc-x64",
      "includePath": [
        "/usr/include",
        "/usr/local/include",
        "${workspaceFolder}",
        "${workspaceFolder}/third_party/CLI11/include",
        "${workspaceFolder}/third_party/imgui",
        "${workspaceFolder}/third_party/ELFIO",
        "${workspaceFolder}/third_party/stb",
        "${workspaceFolder}/third_party/libspng/spng",
        "${workspaceFolder}/bazel-mouse/external/com_google_googletest/googletest/include",
        "/opt/avr-toolchain/avr/include"
      ]
    }
  ],
  "version": 4
}
